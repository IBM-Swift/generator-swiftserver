{
    "services": [
<% Object.keys(services).forEach(function(serviceType, serviceIndex) { -%>
<% services[serviceType].forEach(function(serviceDef, serviceTypeIndex) { -%>
<% if(serviceIndex != 0 || serviceTypeIndex != 0) { -%>,<% }-%>
      <%-include(helpers.getFragmentForService(serviceType), { type: serviceType, service: serviceDef }); -%>
<% }); -%>
<% }); -%>
    ],
    "logger": "<%- config.logger || 'helium' %>",
    "port": "<%- config.port || 8090 %>"
}
